{
  "name": "express-knex-pg-example",
  "version": "1.0.0",
  "description": "Express + Knex + pg example with basic CRUD endpoints",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --watch knexfile.js src/server.js",
    "migrate": "npx knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.js",
    "migrate:make": "npx knex migrate:make"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "knex": "^2.5.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}